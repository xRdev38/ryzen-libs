{
  "name": "ryzen",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "nx": "node ./node_modules/@nrwl/cli/bin/nx",
    "build:all": "nx run-many --target=build-with-task --all --parallel",
    "lint:all": "nx run-many --target=lint --all --parallel",
    "affected": "nx affected",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "copy:all": "nx run-many --target=copy",
    "affected:dep-graph": "nx affected:dep-graph",
    "version:all": "nx run @ryzen/source:local-version",
    "affected:publish": "nx affected --target publish",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "prepare": "husky install",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@nrwl/nx-cloud": "^16.1.1",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.0"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-angular": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jscutlery/semver": "^3.0.0",
    "@nrwl/webpack": "16.5.5",
    "@nx/eslint-plugin": "16.5.5",
    "@nx/jest": "16.5.5",
    "@nx/js": "16.5.5",
    "@nx/linter": "16.5.5",
    "@nx/workspace": "^16.5.5",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "aurelia-binding": "^2.5.4",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-dependency-injection": "^1.5.2",
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-framework": "^1.3.1",
    "aurelia-loader-webpack": "^2.2.1",
    "aurelia-polyfills": "^1.3.4",
    "aurelia-webpack-plugin": "^4.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "16.5.5",
    "nx-cloud": "16.1.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "verdaccio": "^5.0.4",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
